{
    "name": "restaurant-frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@headlessui/react": "^1.7.17",
        "@heroicons/react": "^2.0.18",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.6.0",
        "date-fns": "^2.30.0",
        "lucide-react": "^0.263.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.1",
        "react-router-dom": "^6.8.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "populate": "node populate-db.js",
        "populate:dev": "node populate-db.js restaurant_sample_data.json restaurant_dev.sql",
        "populate:prod": "node populate-db.js restaurant_sample_data.json restaurant_prod.sql",
        "validate": "node -e \"const pop = require('./populate-db.js'); const p = new pop('restaurant_sample_data.json'); p.validateData(); console.log('Validation completed')\"",
        "stats": "node -e \"const pop = require('./populate-db.js'); const p = new pop('restaurant_sample_data.json'); console.log(JSON.stringify(p.getStatistics(), null, 2))\"",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:8080",
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17"
    }
}